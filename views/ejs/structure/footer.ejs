        <% if(locals.selected){ -%>
                <% if(selected == "item"){ -%>
                        <% if(user.SUPERUSER){ -%>
                                <script src="/default/item/s/s_utility.js"></script>
                                <% if(type == "gangs"){ -%>
                                        <script src="/default/item/s/s_gang.js"></script>
                                <% }else if(type == "materials"){ -%>
                                        <script src="/default/item/s/s_material.js"></script>
                                <% }else if(type == "transports"){ -%>
                                        <script src="/default/item/s/s_transport.js"></script>
                                <% }else if(type == "machineries"){ -%>
                                        <script src="/default/item/s/s_machinery.js"></script>
                                <% }else if(type == "su_items"){ -%>
                                        <script src="/default/item/s/s_all.js"></script>
                                <% }-%>
                        <% } else {-%>
                                <script src="/default/item/u/u_utility.js"></script>
                                <% if(type == "u_gangs"){ -%>
                                        <script src="/default/item/u/u_gang.js"></script>
                                <% }else if(type == "u_materials"){ -%>
                                        <script src="/default/item/u/u_material.js"></script>
                                <% }else if(type == "u_transports"){ -%>
                                        <script src="/default/item/u/u_transport.js"></script>
                                <% }else if(type == "u_machineries"){ -%>
                                        <script src="/default/item/u/u_machinery.js"></script>
                                <% }-%>
                        <% }-%>
                <% }else if(selected == "salary"){ -%>
                        <% if(user.SUPERUSER){ -%>
                                <script src="/default/salary/s_salary.js"></script>
                        <% } else {-%>
                                <script src="/default/salary/u_salary.js"></script>
                        <% }-%>
                <% }else if(selected == "char"){ -%>
                        <% if(user.SUPERUSER){ -%>
                                <script src="/default/group/s_groups.js"></script>
                        <% } else {-%>
                                <script src="/default/group/u_groups.js"></script>
                        <% }-%>
                <% } else if(selected == "budget-all"){ -%>
                        <script src="/quote/budgets.js"></script>
                <% } else if(selected == "budget-one"){ -%>
                        <script src="/quote/budget.js"></script>
                <% } else if(selected == "apu"){ -%>
                        <script src="/quote/apu.js"></script>
                <% } else if(selected == "workers"){ -%>
                        <script src="/quote/worker.js"></script>
                <% }-%>-%>
        <% }-%>

